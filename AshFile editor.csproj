<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <RootNamespace>AshFile_editor</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
	<Nullable>enable</Nullable>
	
	<Version>3.1.1</Version>
    <Authors>siljam</Authors> 
    <Company>AshProject</Company>
	<PackageDescription>Simple editor for AshFiles</PackageDescription>
	<RepositoryUrl>https://github.com/siljamdev/AshFile-Editor</RepositoryUrl>
	
	<ApplicationIcon>res/icon.ico</ApplicationIcon>
	
	<PublishSingleFile>true</PublishSingleFile>
	<SelfContained>true</SelfContained>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AshLib" Version="3.3.1" />
  </ItemGroup>

</Project>
